{
    "name": "dontdrinkandroot/api-platform-bundle",
    "description": "Extensions for API Platform",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\ApiPlatformBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "api-platform/core": "^2.0",
        "dontdrinkandroot/common": "dev-master",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/security-core": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/framework-bundle": "^4.4",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/twig-bundle": "^4.4"
    },
    "suggest": {
    },
    "config": {
        "sort-packages": true
    }
}
