{
    "name": "dontdrinkandroot/api-platform-bundle",
    "description": "Extensions for API Platform",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\ApiPlatformBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\ApiPlatformBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7.8",
        "dontdrinkandroot/common": "^0.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/orm": "^2.14",
        "liip/test-fixtures-bundle": "^2.5",
        "phpunit/phpunit": "^9.5.28",
        "symfony/browser-kit": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/validator": "^5.4"
    },
    "suggest": {
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "5.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
