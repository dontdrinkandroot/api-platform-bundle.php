{
    "name": "dontdrinkandroot/api-platform-bundle",
    "description": "Extensions for API Platform",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\ApiPlatformBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.6",
        "dontdrinkandroot/common": "dev-master",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/dbal": "^2.0",
        "doctrine/doctrine-bundle": "^2.0.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "liip/test-fixtures-bundle": "^2.2",
        "phpunit/phpunit": "^9.5.11",
        "symfony/browser-kit": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4"
    },
    "suggest": {
    },
    "config": {
        "sort-packages": true
    }
}
